<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FilletImageView">
        <attr name="round_leftup" format="dimension" />
        <attr name="round_rightup" format="dimension" />
        <attr name="round_leftdown" format="dimension" />
        <attr name="round_rightdown" format="dimension" />
    </declare-styleable>


    <attr name="cropImageStyle" format="reference" />

    <declare-styleable name="CropImageView">
        <attr name="highlightColor" format="reference|color" />
        <attr name="showThirds" format="boolean" />
        <attr name="showHandles">
            <enum name="changing" value="0" />
            <enum name="always" value="1" />
            <enum name="never" value="2" />
        </attr>
    </declare-styleable>
</resources>